  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "2.2.0",
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "9.8.0",
    "concurrently": "5.2.0",
    "cssnano": "4.1.10",
    "postcss-cli": "7.1.1",
    "tailwindcss": "1.4.6"
